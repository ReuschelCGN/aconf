{
    "atlas: update all apps":
    [
        {
        "TYPE": "jobType.PASSTHROUGH",
        "SYNTAX": "su -c '/system/bin/atlas.sh -ua'"
        }
    ],

    "atlas: downgrade pogo":
    [
        {
        "TYPE": "jobType.PASSTHROUGH",
        "SYNTAX": "su -c '/system/bin/atlas.sh -dp'"
        }
    ],

    "atlas: update rgc settings":
    [
        {
        "TYPE": "jobType.PASSTHROUGH",
        "SYNTAX": "su -c '/system/bin/atlas.sh -cr'"
        }
    ],

    "atlas: install atlas config":
    [
        {
        "TYPE": "jobType.PASSTHROUGH",
        "SYNTAX": "su -c '/system/bin/atlas.sh -ic'"
        }
    ],

    "atlas: send logs to discord channel":
    [
        {
        "TYPE": "jobType.PASSTHROUGH",
        "SYNTAX": "su -c '/system/bin/atlas.sh -sl YOUR_DISCORD_WEBHOOK'"
        }
    ]
}
